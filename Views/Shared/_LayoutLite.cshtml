<!DOCTYPE html>
<html lang="en">
    <head>
        <meta charset="utf-8" />
        <meta name="viewport" content="width=device-width, initial-scale=1.0" />
        <title>@ViewData["Title"] - 後台管理範例</title>
        <link rel="stylesheet" href="@Url.Content("~/lib/jqueryui/themes/base/jquery-ui.min.css")" asp-append-version="true" />
        <link rel="stylesheet" href="@Url.Content("~/lib/bootstrap/dist/css/bootstrap.min.css")" asp-append-version="true" />
        <link rel="stylesheet" href="@Url.Content("~/lib/bootstrap-icons/font/bootstrap-icons.min.css")" asp-append-version="true" />
        <link rel="stylesheet" href="@Url.Content("~/lib/datepicker/datepicker.css")" asp-append-version="true" />
        <link rel="stylesheet" href="@Url.Content("~/lib/font-awesome/css/all.css")" asp-append-version="true" />
        <link rel="stylesheet" href="@Url.Content("~/lib/FullCalendar/5.11.0/lib/main.min.css")" asp-append-version="true" />
        <link rel="stylesheet" href="@Url.Content("~/lib/bootstrap-select/css/bootstrap-select.min.css")" asp-append-version="true" />
        <link rel="stylesheet" href="@Url.Content("~/css/site.css")" asp-append-version="true" />
        @await RenderSectionAsync("Styles", required: false)
        <script src="@Url.Content("~/lib/jquery/dist/jquery.min.js")" asp-append-version="true"></script>
        <script src="@Url.Content("~/lib/jqueryui/jquery-ui.min.js")" asp-append-version="true"></script>
        <script src="@Url.Content("~/lib/bootstrap/dist/js/bootstrap.bundle.min.js")" asp-append-version="true"></script>
        <script src="@Url.Content("~/lib/bootstrap-select/js/bootstrap-select.min.js")" asp-append-version="true"></script>
        <script src="@Url.Content("~/lib/bootstrap-select/js/i18n/defaults-zh_TW.min.js")" asp-append-version="true"></script>
        <script src="@Url.Content("~/lib/font-awesome/js/all.js")" asp-append-version="true"></script>
        <script src="@Url.Content("~/lib/FullCalendar/5.11.0/lib/main.min.js")" asp-append-version="true"></script>
        <script src="@Url.Content("~/lib/FullCalendar/5.11.0/lib/locales-all.min.js")" asp-append-version="true"></script>
        <script src="@Url.Content("~/lib/FullCalendar/5.11.0/examples/js/theme-chooser.js")" asp-append-version="true"></script>
        <script src="@Url.Content("~/lib/moment.js/moment.min.js")" asp-append-version="true"></script>
        <script src="@Url.Content("~/lib/moment.js/locale/zh-tw.min.js")" asp-append-version="true"></script>
        <script src="@Url.Content("~/lib/sweetalert/sweetalert.min.js")" asp-append-version="true"></script>
        <script src="@Url.Content("~/lib/datepicker/datepicker.js")" asp-append-version="true"></script>
        <script src="@Url.Content("~/lib/datepicker/datepicker-zh-TW.js")" asp-append-version="true"></script>
        <script src="@Url.Content("~/js/site.js")" asp-append-version="true"></script>
        @await RenderSectionAsync("Scripts", required: false)
    </head>

    <body>
        <div class="container">
            <main role="main" class="pb-3">
            @RenderBody()
            </main>
        </div>

        <footer class="border-top footer text-muted">
            <div class="container">
                &copy; 2023 - myapp - <a asp-area="" asp-controller="Home" asp-action="Privacy">Privacy</a>
            </div>
        </footer>
        @await RenderSectionAsync("ScriptsEnd", required: false)
    </body>
</html>